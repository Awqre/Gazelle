{
  "name": "orpheusnet/gazelle",
  "description": "Web framework for private BitTorrent trackers using Ocelot",
  "type": "project",
  "license": "Unlicense",
  "authors": [
    {
      "name": "What.CD"
    },
    {
      "name": "Apollo"
    },
    {
      "name": "Orpheus"
    }
  ],
  "autoload": {
    "psr-4": {
      "Gazelle\\": "app/"
    },
    "classmap": ["classes/"]
  },
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": "^8.2",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mysqli": "*",
    "ext-openssl": "*",
    "d11wtq/boris": "^v1.0.10",
    "doersf/bitcoin-php": "^2.0",
    "endroid/qr-code": "^4.8.1",
    "hashids/hashids": "^4.1.0",
    "maennchen/zipstream-php": "^2.4.0",
    "monero-integrations/monerophp": "dev-master",
    "orpheusnet/bencode-torrent": "^1.3.0",
    "orpheusnet/logchecker": "^0.12.0",
    "robmorgan/phinx": "^0.12.13",
    "robthree/twofactorauth": "^1.8.2",
    "twig/twig": "^2.15.4",
    "whichbrowser/parser": "^2.1.7"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.10.50",
    "phpunit/php-code-coverage": "^9.2.30",
    "phpunit/phpunit": "^9.6.15",
    "rector/rector": "^0.18.13",
    "squizlabs/php_codesniffer": "^3"
  },
  "scripts": {
    "phpstan": "phpstan analyse",
    "test": [
        "Composer\\Config::disableProcessTimeout",
        "phpunit"
    ]
  }
}
